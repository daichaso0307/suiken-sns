<div class="main users-index">
  <div class="container">
    <h1 class="users-heading">ユーザー一覧</h1>
    <% @users.each do |user| %>
      <div class="users-index-item">
        <div class="user-right">
          <p><%= user.name %></p>
        </div>
      </div>
    <% end %>
    </br>
    <%= link_to '戻る', :back %>
  </div>
</div>